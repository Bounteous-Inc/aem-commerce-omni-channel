<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="WeTelco AEM Mobile article page template"
    jcr:primaryType="cq:Template"
    jcr:title="WeTelco AEM Mobile article page template"
    allowedParents="[/apps/weTelco/weTelco/templates/issue]"
    allowedPaths="[/content/publications(/.*)?,/content/mobileapps(/.*)?]"
    ranking="{Long}1">
    <jcr:content
        cq:designPath="/etc/designs/phonegap/weTelco/weTelco"
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="weTelco/weTelco/components/ng-ionic-article-page"
        dps-resourceType="dps:Article"
        dps-smoothScrolling="always">
        <content-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys">
            <image
                jcr:created="{Date}2016-02-14T18:01:51.655+11:00"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2016-02-14T18:01:58.264+11:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="mobileapps/components/image"
                fileReference=""/>
            <ng-product
                jcr:created="{Date}2013-08-29T10:15:06.985Z"
                jcr:createdBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="weTelco/weTelco/components/ng-product"
                frameworkType="angular">
                <ng-image
                    jcr:created="{Date}2013-08-29T10:15:06.985Z"
                    jcr:createdBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="weTelco/weTelco/components/angular/ng-image"
                    fileReference="/content/dam/geometrixx-outdoors/products/footwear/Nairobi Blue.jpg"
                    frameworkType="angular"/>
            </ng-product>
        </content-par>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone-portrait
                    jcr:primaryType="nt:unstructured"
                    title="Phone Portrait"
                    width="480"/>
                <phone-land
                    jcr:primaryType="nt:unstructured"
                    title="Phone Landscape"
                    width="767"/>
                <tablet-portrait
                    jcr:primaryType="nt:unstructured"
                    title="Tablet Portrait"
                    width="979"/>
                <tablet-land
                    jcr:primaryType="nt:unstructured"
                    title="Tablet Landscape"
                    width="1199"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
