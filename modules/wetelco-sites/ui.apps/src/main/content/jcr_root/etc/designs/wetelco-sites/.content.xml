<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <images/>
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="WeTelco Sites Design"
        sling:resourceType="wcm/core/components/designer"
        addToCartRedirect="/content/geometrixx-outdoors/en/user/cart"
        cartErrorRedirect="/content/geometrixx-outdoors/en/user/cart/product-not-found">
        <page jcr:primaryType="nt:unstructured">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys"
                components="[group:wetelco-sites]">
                <colctrl
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wetelco-sites/components/content/colctrl"
                    layouts="2;cq-colctrl-lt0&#x9;2 Columns (50%, 50%)"/>
                <cq:authoring jcr:primaryType="nt:unstructured">
                    <assetToComponentMapping jcr:primaryType="nt:unstructured">
                        <image
                            jcr:primaryType="nt:unstructured"
                            assetGroup="media"
                            assetMimetype="image/*"
                            droptarget="image"
                            resourceType="wetelco-sites/components/content/image"
                            type="Images"/>
                    </assetToComponentMapping>
                </cq:authoring>
            </par>
            <logo
                jcr:lastModified="{Date}2014-08-21T23:49:27.339-07:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wetelco-sites/components/content/logo"
                imageReference="/content/dam/wetelco-sites/logo/wetelco_logo.png"
                imageRotate="0"
                maxWidth="165">
                <image/>
            </logo>
        </page>
        <productpage jcr:primaryType="nt:unstructured">
            <par
                jcr:lastModified="{Date}2013-01-14T12:49:28.919+01:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/parsys">
                <product
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wetelco-sites/components/content/product"
                    addToCartRedirect="/content/wetelco-sites/en/personal/myaccount/cart"
                    cartErrorRedirect="/content/wetelco-sites/en/personal/myaccount/product-not-found">
                    <image
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wetelco-sites/components/content/product/image"
                        maxWidth="400"/>
                </product>
                <image
                    jcr:primaryType="nt:unstructured"
                    maxHeight=""
                    maxWidth=""/>
                <product_tabs jcr:primaryType="nt:unstructured">
                    <reviews
                        jcr:lastModified="{Date}2015-01-29T10:56:21.220+01:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="social/reviews/components/hbs/reviews"
                        acceptFileTypes=".pdf,.jpg,.jpeg,.zip"
                        allowCloseComments="true"
                        allowDeleteComments="true"
                        allowDenyComments="true"
                        allowedRatings="[Overall,Pricing,Quality]"
                        allowFileUploads="true"
                        allowFlagComments="true"
                        allowRepliesToComments="{Boolean}true"
                        allowVoting="true"
                        flagModerateThreshold="1"
                        flagViewThreshold="5"
                        maxFileSize="104857600"
                        maxMessageLength="4096"
                        requiredRatings="Overall"
                        rteEnabled="true"
                        useFlagReasonList="true"/>
                </product_tabs>
            </par>
        </productpage>
    </jcr:content>
    <clientlib-all/>
    <clientlib-site/>
    <favicon.ico/>
</jcr:root>
